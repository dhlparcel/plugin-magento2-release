<?xml version="1.0"?>
<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <policy id="script-src">
            <values>
                <value id="dhlecommercenl-servicepoint-locator" type="host">https://static.dhlecommerce.nl</value>
                <value id="dhlecommercenl-servicepoint-locator-gmaps" type="host">https://maps.googleapis.com</value>
            </values>
        </policy>
        <policy id="style-src">
            <values>
                <value id="dhlecommercenl-servicepoint-locator" type="host">https://static.dhlecommerce.nl</value>
                <value id="dhlecommercenl-servicepoint-locator-gmaps" type="host">https://maps.googleapis.com</value>
                <value id="dhlecommercenl-servicepoint-locator-gmaps-fonts" type="host">https://fonts.googleapis.com</value>
            </values>
        </policy>
        <policy id="font-src">
            <values>
                <value id="dhlecommercenl-servicepoint-locator" type="host">https://static.dhlecommerce.nl</value>
                <value id="dhlecommercenl-servicepoint-locator-gmaps-fonts-static" type="host">https://fonts.gstatic.com</value>
            </values>
        </policy>
        <policy id="connect-src">
            <values>
                <value id="dhlecommercenl-api" type="host">https://api-gw.dhlparcel.nl</value>
                <value id="dhlecommercenl-servicepoint-locator" type="host">https://static.dhlecommerce.nl</value>
                <value id="dhlecommercenl-servicepoint-locator-gmaps" type="host">https://maps.googleapis.com</value>
            </values>
        </policy>
        <policy id="img-src">
            <values>
                <value id="dhlecommercenl-servicepoint-locator-gmaps" type="host">https://maps.googleapis.com</value>
                <value id="dhlecommercenl-servicepoint-locator-gmaps-static" type="host">https://maps.gstatic.com</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
